{
 "name": "tiny-pixel/acorn-stripe",
 "type": "package",
 "license": "MIT",
 "description": "Stripe payment gateway integration for Acorn projects",
 "authors": [
  {
   "name": "Kelly Mears",
   "email": "kelly@tinypixel.dev"
  }
 ],
 "keywords": [
  "wordpress",
  "acorn",
  "sage"
 ],
 "support": {
  "issues": "https://github.com/pixelcollective/acorn-stripe/issues"
 },
 "autoload": {
  "psr-4": {
   "TinyPixel\\WordPress\\Stripe\\": "src/"
  }
 },
 "require": {
  "php": ">=7.3",
  "league/omnipay": "^3.0",
  "omnipay/stripe": "^3.0",
  "stripe/stripe-php": "^6.40"
 },
 "extra": {
  "acorn": {
   "providers": [
    "TinyPixel\\WordPress\\Stripe\\Providers\\StripeServiceProvider"
   ]
  }
 }
}
